      "item": [
        {
          "name": "Start Negotiation Session",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "if (pm.response.code === 200) {",
                  "    const response = pm.response.json();",
                  "    if (response.session_id) {",
                  "        pm.collectionVariables.set('session_id', response.session_id);",
                  "        console.log('Session ID saved:', response.session_id);",
                  "    }",
                  "}"
                ],
                "type": "text/javascript"
              }
            }
          ],
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"brand_details\": {\n    \"name\": \"TechFlow Solutions\",\n    \"budget\": 75000,\n    \"budget_currency\": \"INR\",\n    \"goals\": [\"brand awareness\", \"product launch\", \"engagement boost\"],\n    \"target_platforms\": [\"instagram\", \"youtube\"],\n    \"content_requirements\": {\n      \"posts\": 4,\n      \"stories\": 8,\n      \"reels\": 3\n    },\n    \"campaign_duration_days\": 30,\n    \"target_audience\": \"Tech-savvy millennials and Gen Z\",\n    \"brand_guidelines\": \"Modern, professional, tech-focused content with authentic storytelling\",\n    \"brand_location\": \"india\"\n  },\n  \"influencer_profile\": {\n    \"name\": \"TechGuru_Priya\",\n    \"followers\": 185000,\n    \"engagement_rate\": 4.8,\n    \"location\": \"india\",\n    \"platforms\": [\"instagram\", \"youtube\", \"linkedin\"],\n    \"niches\": [\"technology\", \"gadgets\", \"lifestyle\"],\n    \"previous_brand_collaborations\": 32\n  }\n}"
            },
            "url": {
              "raw": "{{base_url}}/negotiation-agent/start",
              "host": ["{{base_url}}"],
              "path": ["negotiation-agent", "start"]
            }
          },
          "response": []
        },
        {
          "name": "Continue Negotiation - Interested Response",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"session_id\": \"{{session_id}}\",\n  \"message\": \"Hi! Thanks for reaching out. I'm really interested in this collaboration with TechFlow Solutions. For this scope of work - 4 posts, 8 stories, and 3 reels - my rates are typically ₹85,000 for the complete package. What do you think?\"\n}"
            },
            "url": {
              "raw": "{{base_url}}/negotiation-agent/continue",
              "host": ["{{base_url}}"],
              "path": ["negotiation-agent", "continue"]
            }
          },
          "response": []
        },
        {
          "name": "Continue Negotiation - Counter Offer",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"session_id\": \"{{session_id}}\",\n  \"message\": \"I appreciate your offer. Could we meet somewhere in the middle at ₹80,000? I really believe this collaboration would be perfect for both of us.\"\n}"
            },
            "url": {
              "raw": "{{base_url}}/negotiation-agent/continue",
              "host": ["{{base_url}}"],
              "path": ["negotiation-agent", "continue"]
            }
          },
          "response": []
        },
        {
          "name": "Continue Negotiation - Agreement",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"session_id\": \"{{session_id}}\",\n  \"message\": \"Perfect! I'm happy to move forward at ₹78,000. Let's create the contract and finalize this partnership!\"\n}"
            },
            "url": {
              "raw": "{{base_url}}/negotiation-agent/continue",
              "host": ["{{base_url}}"],
              "path": ["negotiation-agent", "continue"]
            }
          },
          "response": []
        }
      ]
